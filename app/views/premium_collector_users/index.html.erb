<div class="page-header">
  <h1>Premium Collector User Management</h1>
</div>

<div class="actions">
  <%= link_to %{<i class="icon-plus"></i> New Premium Collector User}.html_safe, new_premium_collector_user_path, class: 'btn' %>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Locked</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, edit_premium_collector_user_path(user) %></td>
        <td><%= user.email %></td>
        <td><%=friendly_boolean user.locked %></td>
      </tr>
    <% end %>
  </tbody>
</table>
